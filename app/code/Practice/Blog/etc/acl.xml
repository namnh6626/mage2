        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Practice_Blog::admin" title="Practice Blog">
                    <resource id="Practice_Blog::blog" title="Blog" sortOrder="20">
                        <resource id="Practice_Blog::all_blog" title="All Practice Blog" />
